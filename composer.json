{
    "name": "andreracodex/bi-exchange",
    "description": "A package to fetch exchange rates from Bank Indonesia",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Tirta Rachmandiri Widiantoro",
            "email": "racodex@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Andreracodex\\BiExchange\\": "src/",
            "AndreRacodex\\BiExchange\\Models\\": "src/Models/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andreracodex\\BiExchange\\FetchExchangeServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "php artisan vendor:publish --provider='AndreRacodex\\BiExchange\\FetchExchangeServiceProvider' --tag=migrations --tag=currency-model"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
