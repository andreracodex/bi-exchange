{
    "name": "andreracodex/bi-exchange",
    "description": "A package to fetch exchange rates from Bank Indonesia",
    "keywords": ["laravel", "exchange", "currency", "rates", "bi", "bank-indonesia", "valas"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tirta Rachmandiri Widiantoro",
            "email": "racodex@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Andreracodex\\BiExchange\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andreracodex\\BiExchange\\FetchExchangeServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "php artisan vendor:publish --provider=Andreracodex\\BiExchange\\Providers\\FetchExchangeServiceProvider --tag=migrations",
            "php artisan vendor:publish --provider=Andreracodex\\BiExchange\\Providers\\FetchExchangeServiceProvider --tag=models"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
